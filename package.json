{
  "name": "risichat-back",
  "version": "1.0.0",
  "description": "Risichat backend",
  "scripts": {
    "tsc": "tsc",
    "build": "npm run tsc -- --build",
    "start": "rimraf ./dist/ && concurrently \"npm run build\" \"npm run launch\"",
    "dev": "nodemon",
    "launch": "wait-on ./dist/app.js && node ./dist/app.js",
    "serve": "node ./dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Risiverse/risichat-back.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Risiverse/risichat-back/issues"
  },
  "homepage": "https://github.com/Risiverse/risichat-back#readme",
  "dependencies": {
    "concurrently": "^7.3.0",
    "dotenv": "^16.0.1",
    "mongodb": "^4.6.0",
    "typescript": "^4.6.4",
    "wait-on": "^6.0.1",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/ws": "^8.5.3",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2"
  }
}
